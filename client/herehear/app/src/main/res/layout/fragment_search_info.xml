<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/searchInfo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.search.ui.search.SearchInfoFragment">

    <ImageView
        android:id="@+id/star5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="19dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_toTopOf="@+id/bookDescription"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/star4" />

    <ImageView
        android:id="@+id/star1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="19dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_toTopOf="@+id/bookDescription"
        app:layout_constraintEnd_toStartOf="@+id/star2"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/star4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="19dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_toTopOf="@+id/bookDescription"
        app:layout_constraintEnd_toStartOf="@+id/star5"
        app:layout_constraintStart_toEndOf="@+id/star3" />

    <ImageView
        android:id="@+id/star2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="19dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_toTopOf="@+id/bookDescription"
        app:layout_constraintEnd_toStartOf="@+id/star3"
        app:layout_constraintStart_toEndOf="@+id/star1" />

    <ImageView
        android:id="@+id/searchInfoBack"
        android:layout_width="63dp"
        android:layout_height="57dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="207dp"
        android:layout_marginBottom="313dp"
        android:src="@drawable/back"
        app:layout_constraintBottom_toTopOf="@+id/star1"
        app:layout_constraintEnd_toStartOf="@+id/good"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/good"
        android:layout_width="62dp"
        android:layout_height="53dp"
        android:layout_marginStart="207dp"
        android:layout_marginTop="31dp"
        android:src="@drawable/thumb"
        app:layout_constraintStart_toEndOf="@+id/searchInfoBack"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/bookCover"
        android:layout_width="142dp"
        android:layout_height="184dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="17dp"
        android:src="@drawable/paperbook"
        app:layout_constraintBottom_toTopOf="@+id/bookTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/star3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="19dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_toTopOf="@+id/bookDescription"
        app:layout_constraintEnd_toStartOf="@+id/star4"
        app:layout_constraintStart_toEndOf="@+id/star2" />

    <TextView
        android:id="@+id/bookTitle"
        android:layout_width="149dp"
        android:layout_height="36dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="130dp"
        android:text="책"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookCover" />

    <TextView
        android:id="@+id/bookDescription"
        android:layout_width="259dp"
        android:layout_height="143dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="80dp"
        android:text="책 상세정보"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/star5" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookDescription"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="151dp"
        android:layout_height="30dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/bookDescription" />
</androidx.constraintlayout.widget.ConstraintLayout>
